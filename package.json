{
  "name": "@jamieconnolly/eslint-config",
  "version": "0.5.0",
  "description": "My own personal ESLint configuration",
  "author": "Jamie Connolly <jamie@jamieconnolly.com>",
  "homepage": "https://github.com/jamieconnolly/eslint-config",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "repository": "jamieconnolly/eslint-config",
  "bugs": "https://github.com/jamieconnolly/eslint-config/issues",
  "contributors": "https://github.com/jamieconnolly/eslint-config/graphs/contributors",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "babel -d lib/ src/",
    "lint": "eslint .",
    "test": "babel-tape-runner tests/**/test-*.js | tap-spec",
    "unused-rules": "eslint-find-rules --unused src/index.js",
    "prepublish": "npm run build",
    "pretest": "npm run lint"
  },
  "engine": {
    "node": "4.x || 5.x",
    "npm": ">=2.7 || 3.x"
  },
  "devDependencies": {
    "babel-cli": "^6.6",
    "babel-eslint": "^6.0",
    "babel-preset-es2015": "^6.6",
    "babel-tape-runner": "^2.0",
    "eslint": "^2.8",
    "eslint-find-rules": "^1.4",
    "eslint-plugin-import": "^1.5",
    "eslint-plugin-jsx-a11y": "^1.0",
    "eslint-plugin-react": "^5.0",
    "is-object": "^1.0",
    "tap-spec": "^4.1",
    "tape": "^4.5"
  },
  "peerDependencies": {
    "eslint": "^2.8",
    "eslint-plugin-import": "^1.5",
    "eslint-plugin-jsx-a11y": "^1.0",
    "eslint-plugin-react": "^5.0"
  },
  "babel": {
    "presets": ["es2015"]
  }
}
