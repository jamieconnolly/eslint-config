{
  "name": "@jamieconnolly/eslint-config",
  "version": "3.0.0",
  "description": "My own opinionated ESLint configuration",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "rules:omitted": "eslint-index index.js --status=omitted --docs",
    "rules:status": "eslint-index index.js --format=table",
    "test": "jest"
  },
  "repository": "https://github.com/jamieconnolly/eslint-config",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Jamie Connolly (https://github.com/jamieconnolly)",
  "license": "MIT",
  "bugs": "https://github.com/jamieconnolly/eslint-config/issues",
  "homepage": "https://github.com/jamieconnolly/eslint-config#readme",
  "dependencies": {
    "babel-eslint": "8.2.5",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-promise": "3.5.0"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-index": "^1.3.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-node": "^5.1.0",
    "is-plain-obj": "^1.1.0",
    "jest": "^22.0.0"
  },
  "peerDependencies": {
    "eslint": "^4.8.0"
  },
  "engines": {
    "node": ">=6"
  }
}
